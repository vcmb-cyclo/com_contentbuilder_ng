{
    "$schema": "https://developer.joomla.org/schemas/json-schema/web_assets.json",
    "name": "com_contentbuilder_ng",
    "version": "6.1.0-RC6",
    "description": "Assets pour le composant ContentBuilder_ng",
    "assets": [
        {
            "name": "com_contentbuilder_ng.admin-toolbar",
            "type": "style",
            "uri": "com_contentbuilder_ng/css/admin-toolbar.css"
        },
        {
            "name": "com_contentbuilder_ng.system",
            "type": "style",
            "uri": "com_contentbuilder_ng/css/system.css"
        },
        {
            "name": "com_contentbuilder_ng.contentbuilder_ng",
            "type": "script",
            "uri": "com_contentbuilder_ng/js/contentbuilder_ng.js",
            "dependencies": [
                "core"
            ],
            "attributes": {
                "type": "module"
            }
        },
        {
            "name": "com_contentbuilder_ng.contentbuilder",
            "type": "script",
            "uri": "com_contentbuilder_ng/js/contentbuilder_ng.js",
            "dependencies": [
                "core"
            ],
            "attributes": {
                "type": "module"
            }
        },
        {
            "name": "com_contentbuilder_ng.jscolor",
            "type": "script",
            "uri": "com_contentbuilder_ng/js/jscolor/jscolor.js",
            "dependencies": [
                "core"
            ]
        },
        {
            "name": "com_contentbuilder_ng.coloris.css",
            "type": "style",
            "uri": "https://cdn.jsdelivr.net/npm/@melloware/coloris@0.25.0/dist/coloris.min.css"
        },
        {
            "name": "com_contentbuilder_ng.coloris.js",
            "type": "script",
            "uri": "https://cdn.jsdelivr.net/npm/@melloware/coloris@0.25.0/dist/umd/coloris.min.js",
            "dependencies": [
                "core"
            ],
            "attributes": {
                "defer": true
            }
        }
    ]
}
